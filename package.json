{
  "name": "@hhangular/row-actions",
  "version": "3.0.0",
  "author": "Softwarity",
  "license": "MIT",
  "description": "Angular Material table row actions component with collapsible toolbar",
  "repository": {
    "type": "git",
    "url": "https://github.com/hhangular/row-actions.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "angular",
    "material",
    "table",
    "row-actions",
    "component"
  ],
  "scripts": {
    "ng": "ng",
    "build": "ng build row-actions",
    "build:demo": "ng build demo",
    "start:demo": "ng serve demo",
    "cp:archway-ui": "cp -r dist/* ../../softwarity/archway/archway-ui/node_modules/@hhangular/row-actions/",
    "watch": "ng build row-actions --watch --configuration development",
    "test": "cross-env CHROME_BIN='/usr/bin/google-chrome' ng test",
    "test:ci": "ng test --no-watch --browsers=ChromeHeadless"
  },
  "dependencies": {
    "tslib": "^2.6.2"
  },
  "peerDependencies": {
    "@angular/animations": ">=19.0.0",
    "@angular/cdk": ">=19.0.0",
    "@angular/common": ">=19.0.0",
    "@angular/material": ">=19.0.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.0",
    "@angular/animations": "^19.2.0",
    "@angular/cdk": "^19.2.0",
    "@angular/cli": "^19.2.0",
    "@angular/common": "^19.2.0",
    "@angular/compiler": "^19.2.0",
    "@angular/compiler-cli": "^19.2.0",
    "@angular/core": "^19.2.0",
    "@angular/forms": "^19.2.0",
    "@angular/material": "^19.2.0",
    "@angular/platform-browser": "^19.2.0",
    "@angular/platform-browser-dynamic": "^19.2.0",
    "@angular/router": "^19.2.0",
    "@hhangular/row-actions": "file:dist",
    "@types/jasmine": "~4.6.0",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.2",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^19.2.0",
    "rxjs": "~7.8.1",
    "typescript": "~5.6.0",
    "zone.js": "~0.15.0"
  },
  "sideEffects": false
}
